# Neo4j Authentication
NEO4J_USERNAME=neo4j
NEO4J_PASSWORD=yourpassword
NEO4J_AUTH=${NEO4J_USERNAME}/${NEO4J_PASSWORD}

# User and Group IDs for container permissions
# Default values that should be replaced with actual user/group IDs
USER_ID=1000
GROUP_ID=1000

# Neo4j Configuration
# Memory settings - reduce these values if needed
NEO4J_HEAP_MEMORY_INITIAL=4G
NEO4J_HEAP_MEMORY_MAX=4G
NEO4J_PAGE_CACHE=2G 